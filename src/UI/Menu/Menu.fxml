<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.text.Font?>
<GridPane alignment="center" hgap="10" prefHeight="720.0" prefWidth="1280.0" vgap="10" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="UI.Menu.Controller_Menu">
   <children>
      <SplitPane prefHeight="720.0" prefWidth="1280.0">
        <items>
          <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="720.0" prefWidth="1280.0" styleClass="anchor">
               <children>
                  <Button fx:id="Btn_Play" layoutX="98.0" layoutY="115.0" mnemonicParsing="false" onMouseClicked="#Change_Play" prefHeight="100.0" prefWidth="223.0" text="Play">
                     <font>
                        <Font size="24.0" />
                     </font>
                  </Button>
                  <Button fx:id="Btn_How" layoutX="105.0" layoutY="348.0" mnemonicParsing="false" onMouseClicked="#Change_How" prefHeight="100.0" prefWidth="200.0" text="How to Play">
                     <font>
                        <Font size="24.0" />
                     </font>
                  </Button>
                  <Button fx:id="Btn_Exit" layoutX="107.0" layoutY="499.0" mnemonicParsing="false" onMouseClicked="#Change_Exit" prefHeight="100.0" prefWidth="200.0" text="Exit">
                     <font>
                        <Font size="24.0" />
                     </font>
                  </Button>
               </children>
            </AnchorPane>
        </items>
      </SplitPane>
   </children>
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
